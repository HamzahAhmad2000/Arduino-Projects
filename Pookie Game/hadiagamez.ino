// Function to turn on a box with a specific color (3x3 section of LEDs)
void turnOnBoxWithColor(int boxIndex) {
    for (int i = 0; i < BOX_SIZE; i++) {
        strip.setPixelColor(boxLEDs[boxIndex][i], strip.Color(255, 0, 0)); // Red
    }
    strip.show();
}

