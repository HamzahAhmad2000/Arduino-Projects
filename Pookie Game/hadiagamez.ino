// Function to clear all boxes
void clearAllBoxes() {
    strip.clear();
    strip.show();
}

// Function to update the score LEDs based on the score
void updateScoreLEDs(int score) {
    scoreStrip.clear();
    for (int i = 0; i < score; i++) {
        if (i < 3) {
            // Turn on one LED per score for scores 1 to 3
            scoreStrip.setPixelColor(i, scoreStrip.Color(255, 0, 0)); // Red
        } else {
            // Turn on two LEDs per score for scores 4 to 15
            int index = (i - 3) * 2 + 3;
            if (index < 27) {
                scoreStrip.setPixelColor(index, scoreStrip.Color(255, 0, 0)); // Red
                scoreStrip.setPixelColor(index + 1, scoreStrip.Color(255, 0, 0)); // Red
            }
        }
    }
    scoreStrip.show();
}

void setup() {
    // Initialize serial communication
    Serial.begin(9600);

    // Initialize sensor pins
    for (int i = 0; i < NUM_SENSORS; i++) {
        pinMode(SENSOR_START_PIN + i, INPUT);
    }
    srand(time(0));

    // Initialize button pins
    pinMode(BUTTON_PIN_1, INPUT_PULLUP);
    pinMode(BUTTON_PIN_2, INPUT_PULLUP);
    pinMode(BUTTON_PIN_3, INPUT_PULLUP);

    // Initialize LED strips
    strip.begin();
    strip.show(); // Initialize all pixels to 'off'

    scoreStrip.begin();
    updateScoreLEDs(score);


    if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);}
    delay(2000); // Pause for 2 seconds

    display.clearDisplay();  // Clear the buffer

// Displays FIRSTarray on the screen
    display.drawBitmap(0, 0, epd_bitmap_normal_left, 128, 64, 1);
    display.display();
}

