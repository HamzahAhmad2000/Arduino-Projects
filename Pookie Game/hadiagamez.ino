void game1() {
    bool allBoxesFilled = true;
    static int lastScore = score;
    bool Game = true;
    int filledBox = 0;
    while (Game) {
      
    
        for (int i = 0; i < NUM_SENSORS; i++) {
            if (digitalRead(SENSOR_START_PIN + i) == LOW) {
                Serial.print("Sensor: ");
                Serial.println(i);
                if (!boxFilled[i]) {
                    turnOnBoxWithColor(i); // Red color
                    if (score < 15) {
                        score++;
                        updateScoreLEDs(score);
                    }
                    filledBox++;
                    boxFilled[i] = true;
                }
            }
            allBoxesFilled &= boxFilled[i];
        }

        if (score != lastScore) {
            Serial.print("Score: ");
            Serial.println(score);
            lastScore = score;
        }

        if (filledBox == 16) {
            Game = false;
        }

        delay(100); // Small delay to debounce sensor readings
    }
    clearAllBoxes();
    memset(boxFilled, 0, sizeof(boxFilled));
    delay(2000);
}



